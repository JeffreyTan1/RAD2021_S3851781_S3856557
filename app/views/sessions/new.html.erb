<% flash.each do |type, msg| %>
  <div>
    <%= msg %>
  </div>
<% end %>

<h1>Log In</h1>
<%= form_with url: new_session_path do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </div>

  <div class="actions">
    <%= f.submit 'Log In' %>
  </div>
<% end %>
<%= link_to 'Forgot password?', root_path %>



<br><br>
<%= link_to 'New to us? Signup.', new_user_path %>